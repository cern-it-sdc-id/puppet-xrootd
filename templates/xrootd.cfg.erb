# This file is managed by Puppet. Changes will be undone with the next
# synchronization.

<% if @setenv -%>
  <%- @setenv.sort.each do |key,val| -%>
setenv <%= key %>=<%= val %>
  <%- end -%>
<% end -%>

<% if @set -%>
  <%- @set.sort.each do |key,val| -%>
set <%= key %>=<%= val %>
  <%- end -%>
<% end -%>

<% if @params -%>
  <%- @params.sort.each do |key,val| -%>
    <%- if val.respond_to? :each -%>
      <%- val.each do |v| -%>
<%= key %> <%= v %>
      <%- end -%>
    <%- else -%>
<%= key %> <%= val %>
    <%- end -%>
  <%- end -%>
<% end -%>
